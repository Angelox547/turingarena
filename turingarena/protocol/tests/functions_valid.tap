interface functions_valid {
    function no_args();
    function args(int a, int b);
    function no_return_value(int a);
    function return_value(int a) -> int;

    main {
        call no_args();
        call args(0, 0);
        call no_return_value(0);
        var int x;
        call return_value(0) -> x;
        output x;
    }
}
